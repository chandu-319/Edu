{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "rewrites": [
        {
            "src": "/public/(.*)",
            "dest": "/public/$1"
          },
      {
        "source": "/(.*)",
        "destination": "/app.js"
      }
    ]

  }
  