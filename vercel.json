{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.js"
      },
      {
        "src": "/public/(.*)",
        "dest": "/public/$1"
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Content-Security-Policy",
            "value": "default-src 'none'; font-src 'self' https://fonts.gstatic.com https://edu-nkdh.vercel.app;"
          }
        ]
      }
    ]
  }
  