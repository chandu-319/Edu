{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "rewrites": [
      {
        "source": "/(.*).css",
        "destination": "/public/$1.css"
      },
      {
        "source": "/(.*).png",
        "destination": "/public/$1.jpg"
      },
      {
        "source": "/(.*).jpeg",
        "destination": "/public/$1.jpeg"
      },
      {
        "source": "/(.*)",
        "destination": "/app.js"
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Content-Security-Policy",
            "value": "default-src 'none'; style-src 'self' https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css https://edu-ycmg.vercel.app; font-src 'self' https://edu-ycmg.vercel.app data:; img-src 'self' https://edu-ycmg.vercel.app; script-src 'self';"
          }
        ]
      }
    ]
  }
  